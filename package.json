{
  "name": "first-free-subnet",
  "version": "0.1.7",
  "description": "This function should return first available subnet after processing already allocated subnets",
  "main": "lib/index.js",
  "author": "Viljar VÃµidula <viljar@testreel.com>",
  "license": "MIT",
  "dependencies": {
    "neon-cli": "^0.4.0",
    "node-pre-gyp": "amilajack/node-pre-gyp",
    "node-pre-gyp-github": "^1.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViljarVoidula/allocate-free-subnet.git"
  },
  "files": [
    "native/index.node",
    "lib"
  ],
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false --release || neon build --release",
    "package": "node-pre-gyp package",
    "upload-binary": "node-pre-gyp package && node-pre-gyp-github publish",
    "build:windows": "node-pre-gyp package --target_platform=win32 --msvs_version=2018 --target_arch=x64 --update-binary --release",
    "build:linux": "node-pre-gyp package --target_platform=linux --target_arch=x64 --update-binary --release",
    "publish:release": "yarn build:windows && yarn build:linux && node-pre-gyp-github publish --release"
  },
  "binary": {
    "module_name": "index",
    "host": "https://github.com/ViljarVoidula/allocate-free-subnet/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native",
    "pkg_path": "."
  }
}
